[init]
	defaultBranch = main
[github]
	user = rufo
[push]
	default = simple
[credential "https://github.com"]
	helper = !gh auth git-credential
[core]
	excludesfile = /Users/rufo/.gitignore_global
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[pull]
	ff = only
[commit]
	verbose = true
